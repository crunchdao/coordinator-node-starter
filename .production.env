POSTGRES_USER=condorgame
POSTGRES_PASSWORD=condorgame
POSTGRES_DB=condorgame
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
MODEL_RUNNER_NODE_HOST=model-orchestrator
MODEL_RUNNER_NODE_PORT=9091
PROJECT_DIR=~/workspace/condorgame-backend # for building
READONLY_USER_PASSWORD=<readonly_user_password>

# NewRelic Configuration (Optional)
# If NEW_RELIC_LICENSE_KEY is not set, monitoring is disabled
# Get your license key from: https://one.newrelic.com/admin-portal/api-keys/home
NEW_RELIC_LICENSE_KEY=<NEW_RELIC_LICENSE_KEY>

# NewRelic Alerts (for Terraform - runs via docker-compose)
NEW_RELIC_ACCOUNT_ID=<NEW_RELIC_ACCOUNT_ID>
NEW_RELIC_API_KEY=<NEW_RELIC_API_KEY>
NEW_RELIC_REGION=EU

# At least one of these must be set
# NEW_RELIC_NOTIFICATION_EMAIL=alerts@yourcompany.com
# Create a Slack Incoming Webhook: https://api.slack.com/apps → Create App → Incoming Webhooks → Add New Webhook
# NEW_RELIC_SLACK_WEBHOOK_URL=<NEW_RELIC_SLACK_WEBHOOK_URL>
# NEW_RELIC_SLACK_CHANNEL=#alerts